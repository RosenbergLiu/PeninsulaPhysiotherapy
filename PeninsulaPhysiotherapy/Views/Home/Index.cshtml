@{
    ViewData["Title"] = "Home Page";
}
<head>
    <script src="~/js/main.js"></script>
    <link href="~/css/main.css" rel="stylesheet" />
</head>
<div class="image">

    <img src="~/lib/home.jpg"/>
    <h1>Peninsula Physiotherapy</h1>
</div>


<style>

    .image {
        position: relative;
        width: 100%; /* for IE 6 */
        height:100%;
        text-align:center;
    }

    h1 {
        position: absolute;
        top: 30%;
        left: 0;
        width: 100%;
        color:white;
        font-size: 80px;
        font-family:Arial, Helvetica, sans-serif;
    }
</style>
<br />


<div class="container-lg" style="text-align:center">
    <h2>Where to find us</h2>
    <iframe
            height="500"
            style="border:0; width:50%;"
            loading="lazy"
            allowfullscreen
            referrerpolicy="no-referrer-when-downgrade"
            src="https://www.google.com/maps/embed/v1/place?key=AIzaSyCXdfLBloKI6vbPymPV1_R46ypLgboMYgE
    &q=Blowhole+Lookout,Melbourne+VIC&zoom=12">
    </iframe>

</div>




